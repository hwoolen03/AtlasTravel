module(
    name = "tfjs-backend-cpu-custom",
    version = "1.0.0",
    compatibility_level = 1,
)

# Use rules_nodejs (no need for separate TypeScript rules)
bazel_dep(name = "rules_nodejs", version = "6.3.3")

# Declare npm dependencies (if using Bzlmod)
npm = use_extension("@rules_nodejs//nodejs:extensions.bzl", "npm")
npm.npm_translate_lock(
    name = "npm",
    pnpm_lock = "//:pnpm-lock.yaml",  # or package_lock_json = "//:package-lock.json"
)
use_repo(npm, "npm")
